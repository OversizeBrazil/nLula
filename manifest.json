{
    "manifest_version": 3,
    "name": "nLula",
    "description": "A Chrome extension which replaces all images on all pages to Luiz In√°cio Lula da Silva.",
    "version": "1.1",
    "icons": {
        "128": "icon128.png"
    },
    "content_scripts": [
		{
			"matches": ["*://*/*"],
			"js": ["jquery.min.js", "nlula.js"],
			"run_at": "document_end"
		}
	],
    "host_permissions": ["*://*/*"],
    "permissions": [],
    "web_accessible_resources": [
		{
			"resources": ["background/bg1.png"],
			"matches": ["*://*/*"]
		}
	]
}