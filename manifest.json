{
    "manifest_version": 3,
    "name": "nLula",
    "description": "A Chrome extension which replaces all images on all pages to Luiz In√°cio Lula da Silva.",
    "version": "1.1",
    "icons": {
        "128": "icon128.png",
        "48": "icon48.png"
    },
    "content_scripts": [
		{
			"matches": ["*://*/*"],
			"js": ["nlula.js"],
			"run_at": "document_end"
		}
	],
    "permissions": ["storage"],
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    },
    "web_accessible_resources": [
		{
			"resources": ["logo/logo.png"],
			"matches": ["*://*/*"]
		}
	]
}